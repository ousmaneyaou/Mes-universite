<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">
    <!--
        Added the constraints for entity Depot.
    -->
    <changeSet id="20230906012351-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="bachelier_id"
                                 baseTableName="depot"
                                 constraintName="fk_depot__bachelier_id"
                                 referencedColumnNames="id"
                                 referencedTableName="bachelier"/>

        <addForeignKeyConstraint baseColumnNames="dossier_id"
                                 baseTableName="depot"
                                 constraintName="fk_depot__dossier_id"
                                 referencedColumnNames="id"
                                 referencedTableName="dossier"/>
    </changeSet>
</databaseChangeLog>
